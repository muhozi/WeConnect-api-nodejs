{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "WeConnect provides a platform that brings businesses and individuals together",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --coverageDirectory=coverage/jest",
    "test:ci": "npm run test:cov --ci --reporters=default --reporters=jest-junit",
    "start": "nodemon ./index.js --exec babel-node --presets babel-preset-env",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.js\"",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/muhozi/WeConnect-api-nodejs.git"
  },
  "keywords": [
    "WeConnect",
    "api",
    "nodejs"
  ],
  "author": "Emery Muhozi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muhozi/WeConnect-api-nodejs/issues"
  },
  "homepage": "https://github.com/muhozi/WeConnect-api-nodejs#readme",
  "dependencies": {
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "pg": "^7.7.1",
    "sequelize": "^4.42.0",
    "swagger-tools": "^0.10.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.3.0"
  }
}
